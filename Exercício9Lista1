// 9) VPM = L1 * L2 * L3 Volume de Prisma



Algoritmo "Exercício9daLista1"

Var

   L1, L2, L3, volume: real

Inicio
   escreval ("Para calcularmos o volume do prisma, informe o lado 1: ")
   leia (l1)
   escreval ("Agora, informe o lado 2: ")
   leia (l2)
   escreval ("Para finalizarmos a conta, digite o lado 3: ")
   leia (l3)
   
   volume <- l1*l2*l3
   
   escreval ("O volume do prisma é: ", volume)



Fimalgoritmo
